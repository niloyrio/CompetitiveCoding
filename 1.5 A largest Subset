import math
for i in range(int(input())):
    n=int(input())
    l=[]
    m=[]
    for i in range(1,n+1):
        if n%i==0:
            l.append(i)
    import math
    for i in l :
         if i>1:
              if math.sqrt(i).is_integer():
                  m.append(i)
    print(l)
    print(m)
    if len(m)==0:
        print("yes")
    else:
        print("No") 